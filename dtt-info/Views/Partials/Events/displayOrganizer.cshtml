@model IPublishedContent

@{
        if (Model.GetPropertyValue("Title").ToString().Length > 0)
        {
            @Model.GetPropertyValue("Title") <br />
        }
        if (Model.GetPropertyValue("Address").ToString().Length > 0)
        {
            @Model.GetPropertyValue("Address")<br />
        }

        if (Model.GetPropertyValue("City").ToString().Length > 0 && Model.GetPropertyValue("Zip").ToString().Length > 0)
        {
            @Model.GetPropertyValue("Zip")@Html.Raw("&nbsp;")@Model.GetPropertyValue("City")
        }
}

